);\r\n        // if((topicCheck !== \"\") && (currentQuote.quoteText !== quote.quoteText) && !(similarQuotes.includes(currentQuote))) similarQuotes.push(currentQuote);\r\n        console.log(similarQuotes.length);\r\n        currentQuote = quotePile[i];\r\n\r\n        if((currentQuote !== quote) && (currentQuote.author !== quote.author)) {\r\n            sharedWordsLength = compareQuotes(quote, currentQuote);\r\n\r\n            if((sharedWordsLength >= 1) && !(similarQuotes.includes(currentQuote))) {\r\n                similarQuotes.push(currentQuote);\r\n            }\r\n        }\r\n    }\r\n\r\n    if(similarQuotes.length <= 0) return null;\r\n\r\n    return (\r\n        <Card bg={\"theme\"}>\r\n            <Card.Header>Similar Quotes</Card.Header>\r\n            <Card.Body>\r\n                {similarQuotes.map((index) => (\r\n                    <Link to={`/quote/${index._id}`} key={index.quoteText}>\r\n                        <Button variant={\"theme\"} className=\"mb-3 py-2 btn-block\"><i>\"{index.quoteText}\"</i> - {index.author}</Button>\r\n                    </Link>\r\n                ))}\r\n            </Card.Body>\r\n        </Card>\r\n    )\r\n}\r\n\r\nexport default MoreQuotesBy;","C:\\Users\\thees\\Documents\\Programming_Backups\\React Stuff\\react-undoctrination2\\react-undoctrination2\\client\\src\\components\\AuthorPortraitButton\\index.js",[],"C:\\Users\\thees\\Documents\\Programming_Backups\\React Stuff\\react-undoctrination2\\react-undoctrination2\\client\\src\\components\\TopicButton\\index.js",[],["117","118"],"C:\\Users\\thees\\Documents\\Programming_Backups\\React Stuff\\react-undoctrination2\\react-undoctrination2\\client\\src\\pages\\SearchResult.js",[],"C:\\Users\\thees\\Documents\\Programming_Backups\\React Stuff\\react-undoctrination2\\react-undoctrination2\\client\\src\\components\\Results\\index.js",[],"C:\\Users\\thees\\Documents\\Programming_Backups\\React Stuff\\react-undoctrination2\\react-undoctrination2\\client\\src\\components